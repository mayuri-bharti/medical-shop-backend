{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/google",
      "dest": "/api/auth/google.js"
    },
    {
      "src": "/api/auth/google/callback",
      "dest": "/api/auth/google/callback.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
